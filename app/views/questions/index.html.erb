<h1>All Questions</h1>
<table>
  <% @questions.each do |question| %>
  <tr>
    <td>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
              <%= link_to question.title, question %>
          </h4>
        </div>
      </div>
  </td>
  <td>
      <% if question.resolved %>
          Resolved
      <% else %>
          Not Resolved
      <% end %>
  </td>
</tr>
<% end %>
</table>
